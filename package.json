{
  "name": "piroutine-server",
  "version": "3.0.0",
  "description": "A server for a raspberry pi, that can handle 2 directional communication with a client",
  "scripts": {
    "start": "concurrently --kill-others \"npm run server\" \"npm run gpio-client\"",
    "start-legacy": "node server/index.js",
    "gpio-client": "node server/client.js -w",
    "server": "node server/index.js -w",
    "test": "nodemon server/timer/timer.js"
  },
